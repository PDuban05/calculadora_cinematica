<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".head">

    <RelativeLayout
        android:id="@+id/head"
        android:layout_width="match_parent"
        android:layout_height="85dp"
        >


        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:background="@drawable/layouthead"
            android:elevation="4dp">

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:gravity="center">

                <ImageView
                    android:id="@+id/mru_v"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/linea_head"
                    />

                <ImageView

                    android:id="@+id/mrua_v"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/linea_head" />

                <ImageView

                    android:id="@+id/mcu_v"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/linea_head" />

                <ImageView

                    android:id="@+id/mcua_v"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/linea_head" />

                <ImageView
                    android:id="@+id/mpcl_v"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/linea_head" />

                <ImageView

                    android:id="@+id/tv_v"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/linea_head" />

                <ImageView

                    android:id="@+id/ca_v"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/linea_head"
                    android:layout_weight="1"

                    />


            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp">

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/mru_t"
                    style="@style/text_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/MRU_tt"
                    android:textAlignment="center"
                    android:background="@drawable/button_head"

                    />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/mrua_t"
                    style="@style/text_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/MRUA_tt"
                    android:textAlignment="center"
                    android:background="@drawable/button_head"/>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/mcu_t"
                    style="@style/text_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/MCU_tt"
                    android:textAlignment="center"
                    android:background="@drawable/button_head"/>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/mcua_t"
                    style="@style/text_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/MCUA_tt"
                    android:textAlignment="center"
                    android:background="@drawable/button_head"/>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/mpcl_t"
                    style="@style/text_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/MPCL_tt"
                    android:textAlignment="center"
                    android:background="@drawable/button_head"/>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/tv_t"
                    style="@style/text_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:lines="2"
                    android:text="@string/TV_tt"
                    android:textAlignment="center"
                    android:layout_marginTop="-5dp"
                    android:background="@drawable/button_head"/>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/ca_t"
                    style="@style/text_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/CA_tt"
                    android:layout_weight="1"
                    android:textAlignment="center"
                    android:background="@drawable/button_head"
                    />

            </TableRow>


        </TableLayout>

    </RelativeLayout>

</FrameLayout>